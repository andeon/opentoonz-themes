  qproperty-SelectedKeyFrameColor: #af8e73;
  qproperty-IgnoredKeyFrameColor: #cb5765;
  qproperty-SelectedIgnoredKeyFrameColor: #be73a4;
  qproperty-InBetweenColor: #e2dbcc;
  qproperty-InBetweenBorderColor: #ac9f82;
  qproperty-SelectedInBetweenColor: #a8b0bb;
  qproperty-IgnoredInBetweenColor: #c29c92;
  qproperty-SelectedIgnoredInBetweenColor: #928492;
  qproperty-SelectedEmptyColor: rgba(255, 255, 255, 0.5);
  qproperty-SelectedSceneRangeEmptyColor: rgba(255, 255, 255, 0.7);
  qproperty-TextColor: #000;
  qproperty-ColumnHeaderBorderColor: #949494;
}
#ExpressionField {
  background-color: #ffffff;
  border: 1 solid #cccccc;
  margin: 0;
}
/*------------- QSplitter / Menu Fixes --------- */
QSplitter {
  background-color: #ebebeb;
}
QSplitter::handle {
  margin: 0px;
  background-color: #e1e1e1;
}
QSplitter::handle:horizontal {
  border-left: 1px solid #787878;
  border-right: 1px solid #787878;
}
QSplitter::handle:vertical {
  border-top: 1px solid #787878;
  border-bottom: 1px solid #787878;
  padding-left: 1px;
  padding-right: 1px;
}

QMenuBar::item:selected {
  border: 1 solid #7E7E7E;
  background: qlineargradient(x1:1, y1:0, x2:1, y2:1,
  stop:0 #EAEAEA, stop:1 #D2D2D2 );

}
QMenuBar::item:pressed {
  background: qlineargradient(x1:1, y1:0, x2:1, y2:1,
  stop:0 #BFBFBF, stop:1 #C6C6C6 );
}

/*---------------------------------------------------------------------------*/
QMainWindow #MainStackedWidget {
  border: 6px solid #6d6d6d;
  background-color: transparent;
}
